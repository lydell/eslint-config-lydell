{
  "name": "eslint-config-lydell",
  "version": "8.0.0",
  "license": "MIT",
  "author": "Simon Lydell",
  "description": "Kinda strict Prettier-friendly ESLint config.",
  "repository": "lydell/eslint-config-lydell",
  "files": [
    "index.js"
  ],
  "keywords": [
    "eslint",
    "eslintconfig",
    "prettier"
  ],
  "scripts": {
    "test:lint": "eslint .",
    "test:lint-rules": "eslint index.js --config test-config/.eslintrc.js --format json",
    "test:ava": "ava",
    "test:prettier-friendly": "eslint --print-config .eslintrc.test.js | eslint-config-prettier-check",
    "test": "npm run test:lint && npm run test:ava && npm run test:prettier-friendly"
  },
  "devDependencies": {
    "ava": "0.25.0",
    "eslint": "5.0.1",
    "eslint-config-prettier": "2.9.0",
    "eslint-plugin-flowtype": "2.49.3",
    "eslint-plugin-import": "2.13.0",
    "eslint-plugin-jest": "21.17.0",
    "eslint-plugin-prettier": "2.6.1",
    "eslint-plugin-react": "7.10.0",
    "prettier": "1.13.7",
    "rimraf": "2.6.2"
  },
  "peerDependencies": {
    "eslint": ">=4.9.0"
  }
}
